{
  "name": "oceanaware-guardian",
  "version": "1.0.0",
  "description": "AI-powered ocean hazard monitoring and conservation education platform for Bow Seat Ocean Awareness Contest 2026",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "start:dev": "bash start-dev.sh",
    "stop:dev": "bash stop-dev.sh",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:all": "npm run test && cd server && npm test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\"",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "setup:all": "npm install && cd server && npm install && cd .. && cp .env.example .env.local",
    "deploy": "npm run build && vercel --prod",
    "generate:demo-data": "node scripts/generateDemoData.js",
    "start": "node server.js",
    "debug": "node startup-check.js"
  },
  "dependencies": {
    "@azure/cosmos": "^4.5.0",
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@tanstack/react-query": "^5.8.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "^6.7.0",
    "fast-xml-parser": "^5.2.5",
    "firebase": "^10.14.1",
    "framer-motion": "^10.16.0",
    "helmet": "^7.0.0",
    "leaflet": "^1.9.4",
    "localforage": "^1.10.0",
    "lucide-react": "^0.263.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.20.0",
    "react-use-gesture": "^9.1.3",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.8.0",
    "sharp": "^0.32.0",
    "tailwindcss": "^3.3.0",
    "vite": "^4.5.0",
    "web-push": "^3.6.7",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitest/ui": "^0.34.0",
    "c8": "^8.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.54.1",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "vitest": "^0.34.0"
  },
  "keywords": [
    "ocean",
    "conservation",
    "tsunami",
    "coastal-erosion",
    "sea-level-rise",
    "climate-change",
    "ai",
    "computer-vision",
    "ocean-awareness-contest",
    "marine-conservation",
    "bow-seat",
    "ecoquest"
  ],
  "author": "Siddharth Shan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siddharth-shan/oceanaware-guardian.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
